    "motion": "^11.15.0",
    "react": "^19.0.0",
    "react-aria-components": "^1.5.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.0.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@react-router/dev": "^7.0.2",
    "@tailwindcss/vite": "^4.0.0-beta.8",
    "@types/node": "^20",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "cross-env": "^7.0.3",
    "rustywind": "^0.23.1",
    "tailwind-merge": "^2.5.5",
